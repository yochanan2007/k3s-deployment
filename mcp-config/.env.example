# SSH Configuration
SSH_PRIVATE_KEY_PATH=C:/Users/John/.ssh/docker_key

# GitHub API Token
GITHUB_TOKEN=ghp_your_github_token_here

# AdGuard Home
ADGUARD_USERNAME=admin
ADGUARD_PASSWORD=your_adguard_password

# Authentik API Token
# Get from: https://auth.k3s.dahan.house/if/admin/#/core/tokens
AUTHENTIK_API_TOKEN=your_authentik_token_here

# Rancher API Token
# Get from: https://rancher.k3s.dahan.house/dashboard/account
RANCHER_API_TOKEN=token-xxxxx:xxxxxxxxxxxxxxxxxxxxxxxx

# Home Assistant Long-Lived Access Token
# Get from: https://home.k3s.dahan.house/profile -> Long-Lived Access Tokens
HOMEASSISTANT_TOKEN=your_homeassistant_token_here

# n8n API Key
# Get from: https://n8n.k3s.dahan.house/settings/api
N8N_API_KEY=your_n8n_api_key_here

# Portainer Credentials
PORTAINER_USERNAME=admin
PORTAINER_PASSWORD=your_portainer_password

# Nautobot API Token (already set during deployment)
NAUTOBOT_API_TOKEN=0123456789abcdef0123456789abcdef01234567

# PostgreSQL
POSTGRES_USERNAME=superset
POSTGRES_PASSWORD=your_postgres_password
